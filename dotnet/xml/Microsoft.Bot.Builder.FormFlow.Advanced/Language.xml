      <MemberSignature Language="F#" Value="static member GenerateTerms : string * int -&gt; string[]" Usage="Microsoft.Bot.Builder.FormFlow.Advanced.Language.GenerateTerms (phrase, maxLength)" />
      <MemberType>Method</MemberType>
